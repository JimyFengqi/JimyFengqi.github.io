<!-- 备案信息+ -->
<div class="footer-icp">
    <% (theme.site_footer.icp||[]).forEach(function(value) { %>
        <% if (value.on) { %>
          <span id="icp">
            <a href="<%- url_for(value.url) %>" target="_blank"><%= value.text %></a>
          </span>
        <% } else { %>
          <span id="icp">
            <a href="#" target="_blank">暂时没有备案信息</a>
          </span>
        <% } %>
    <% }) %>
</div>